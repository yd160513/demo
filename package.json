{
  "name": "demo",
  "private": true,
  "version": "0.0.0",
  "main": "src/main/main.js",
  "scripts": {
    "dev:web": "vite",
    "dev:pc": "concurrently --kill-others \"npm run dev:web\" \"npm run electron:serve\"",
    "electron:serve": "wait-on http://localhost:5173 && electron .",
    "build:web": "vite build",
    "build:pc": "npm run clean && npm run build:web && electron-builder",
    "clean": "rimraf dist",
    "preinstall": "node scripts/check-node-version.js"
  },
  "engines": {
    "node": "20.17.0",
    "npm": "10.8.2"
  },
  "build": {
    "asar": true,
    "productName": "demo",
    "directories": {
      "output": "dist/pc"
    },
    "files": [
      "dist/web/**/*",
      "src/main/**/*"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-reload": "^2.0.0-alpha.1",
    "element-plus": "^2.8.3",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.79.3",
    "semver": "^7.6.3",
    "vite": "^5.4.7",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "electron-log": "^5.2.0"
  }
}
